                        pos_hint:{'center_x':0.3,'center_y':0.4}
                        size_hint:(1/5,1/8)
                Button:
                        text:'Loans'
                        on_press:root.loans()
                        pos_hint:{'center_x':0.3,'center_y':0.2}
                        size_hint:(1/5,1/8)
                Button:
                        text:'Log Out'
                        on_press:root.manager.current='login'
                        size_hint:(1/8,1/11)
                        pos_hint:{'center_x':0.95,'center_y':0.95}

