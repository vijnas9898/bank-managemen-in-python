<LoginScreen>:
        name:'login'
        BoxLayout:
                canvas.before:
                        Rectangle:
                                pos:self.pos
                                size:self.size
                                source:'lscreen.jpg'
                FloatLayout:
                        size:(1/8,1/8)
                        cols:2
                        Label:
                                text:'Branch ID'
                                font_size:18
                                pos_hint:{'center_x':0.4,'center_y':0.6}
                                size_hint:(1/11,1/18)
                        TextInput:
                                id:bid
                                multiline:False
                                pos_hint:{'center_x':0.6,'center_y':0.6}
                                size_hint:(1/8,1/18)
                        Label:
                                text:'Employee Id'
                                font_size:18
                                pos_hint:{'center_x':0.4,'center_y':0.5}
                                size_hint:(1/11,1/18)
                        TextInput:
                                id:uname
                                multiline:False
                                pos_hint:{'center_x':0.6,'center_y':0.5}
                                size_hint:(1/8,1/18)
                        Label:
                                text:'Password'
                                font_size:18
                                pos_hint:{'center_x':0.4,'center_y':0.4}
                                size_hint:(1/11,1/18)
                        TextInput:
                                id:passw
                                password:True
                                multiline:False
                                pos_hint:{'center_x':0.6,'center_y':0.4}
                                size_hint:(1/8,1/18)
                        Button:
                                text:'Login'
                                on_press:root.verify()
                                pos_hint:{'center_x':0.5,'center_y':0.3}
                                size_hint:(1/11,1/18)

<MenuScreen>:
        name:'menu'
        BoxLayout:
                canvas.before:
                        Rectangle:
                                pos:self.pos
                                size:self.size
                                source:'bg.jpg'
        FloatLayout:
                Button:
                        text: 'Add account'
                        on_press: root.manager.current = 'newacc'
                        pos_hint: {'center_x': 0.3, 'center_y': 0.8}
                        size_hint: (1/5,1/8)
                Button:
                        text:'Modify account'
                        on_press:root.manager.current='modacc'
